
=== .* ===

os:dev
	MODULES
		/dev/.*

os:boot
	MODULES
		/boot/.*

os:linux-kernel
	MODULES
		/boot/.*
		/usr/src/kernels.*
		/usr/lib/modules/.*
		/usr/lib/firmware(/.*)?

os:autogenerated
	MODULES
		/initrd
		/.autorelabel
		/chef-client-test.sh
		/linux_agent.inst
		/var/.updated
		/var/opt
		/usr/sbin/ociudevpersistentnaming
		/lost+found
		/media
		/mnt
		/opt
		/srv

os:cache
	VOLATILES
		/usr/lib/fontconfig/cache/.*
		/usr/lib/kbd/consolefonts/.*\.gz
		/usr/lib/kbd/consoletrans/.*\.trans
		/usr/lib/kbd/keymaps/.*\.gz
		/usr/lib/kbd/unimaps/.*\.uni
		/usr/share/mime/mime.cache
		/usr/share/mime/[-\w]+
		/usr/share/mime/.*\.xml
		/usr/share/locale/.*
		/usr/share/man/.*
		/usr/.*\.pyc
		/usr/.*/__pycache__
		/usr/share/fonts/(.*/)?\.uuid
		/usr/share/X11/fonts/(.*/)?\.uuid
		/etc/.*\.save
		/etc/(passwd|group|shadow|subgid|subuid)-
		/etc/aliases.db
		/.*/\.cache(/.+)
		/.*/\.npm/_cacache(/.+)
		/.*/\.npm/_locks(/.+)
		/.*/\.npm/_logs(/.+)

os:oci-agent
	MODULES
		/opt/unified-monitoring-agent(/.*)?


